{
    "name": "michtio/gcal",
    "description": "oAuth and routing for Google Calendar",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "Google Calendar",
        "gcal"
    ],
    "support": {
        "docs": "https://github.com/michtio/craft-gcal/blob/master/README.md",
        "issues": "https://github.com/michtio/craft-gcal/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Thomas",
            "homepage": "michtio.dev"
        }
    ],
    "require": {
        "craftcms/cms": "^3.1.0",
        "league/oauth2-client": "^2.2",
        "league/oauth2-google": "^3.0",
        "google/apiclient": "^v2.2.0"
    },
    "autoload": {
        "psr-4": {
          "michtio\\gcal\\": "src/"
        }
    },
    "extra": {
        "name": "gcal",
        "handle": "gcal",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/michtio/gcal/master/CHANGELOG.md",
        "components": {
            "gcalService": "michtio\\gcal\\services\\GcalService"
        },
        "class": "michtio\\gcal\\Gcal"
    }
}
